Model RLP_EveningStar_Projectile
{
	Path "Models/Player/Weapons/Projectiles" // Path to model in PK3
	SKIN 0 "PLSSA0.png" // Model index, model file
	MODEL 0 "Tracer.md3" // Model index, texture (can be in any format supported by GZDoom)
	SKIN 1 "PLSSB0.png" // Model index, model file
	MODEL 1 "Tracer.md3" // Model index, texture (can be in any format supported by GZDoom)
	Scale 1.25 1.25 1.25 // Scale values
	PITCHFROMMOMENTUM // A flag that will fix the model pitch for projectiles

	FrameIndex M000 A 0 0 // The sprite lump, sprite frame, model index, frame number
	FrameIndex M000 B 1 0 // The sprite lump, sprite frame, model index, frame number
}
